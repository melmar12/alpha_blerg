<h1>Login</h1>

<%= form_for(:session, role: 'form', url: login_path) do |f| %>

<p>
	<%= f.label :email %><br/>
	<%= f.email_field :email, placeholder: 'enter email', autofocus: true %>
</p>

<p>
	<%= f.label :password %><br/>
	<%= f.password_field :password, autocomplete: 'off' %>
</p>

<p>
	<%= f.submit 'Login' %>
</p>
<% end %>